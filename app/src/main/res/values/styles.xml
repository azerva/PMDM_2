<?xml version="1.0" encoding="utf-8"?>
<resources><style name="Botones" >
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:textColor">@color/white</item>
    <item name="android:layout_margin">10dp</item>
    <item name="android:textStyle">bold</item>
    <item name="android:background">@drawable/design_button_app</item>
</style>
    <style name="main_img_logo" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">250dp</item>
        <item name="android:src">@drawable/logo_zumos</item>
        <item name="android:layout_marginStart">10dp</item>
        <item name="android:layout_marginEnd">10dp</item>
        <item name="android:scaleType">fitXY</item>
    </style>
    <style name="titulo" parent="TextAppearance.AppCompat.Title" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fontFamily">sans-serif-black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="et_registro" parent="TextAppearance.AppCompat.Title" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">10dp</item>

    </style>
    <style name="subTitulo" parent="@android:style/TextAppearance.Large">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/naranja</item>
        <item name="android:textSize">18sp</item>
        <item name="fontFamily">sans-serif-black</item>
        <item name="android:layout_marginTop">8dp</item>
    </style></resources>